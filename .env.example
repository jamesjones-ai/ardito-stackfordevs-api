# StackForDevs API Configuration
# Copy this to .env and fill in your credentials from https://admin.stackfordevs.com

# Credentials (from StackForDevs Admin Dashboard)
STACKFORDEVS_TENANT_ID=your-tenant-id-here
STACKFORDEVS_PROJECT_ID=your-project-id-here
STACKFORDEVS_API_KEY=stk_pub_your-public-key-here
STACKFORDEVS_SECRET_API_KEY=stk_sec_your-secret-key-here

# API Endpoints (Production)
STACKFORDEVS_AUTH_API_URL=https://auth.stackfordevs.com
STACKFORDEVS_NOTIFICATIONS_API_URL=https://notifications.stackfordevs.com
STACKFORDEVS_MAILER_API_URL=https://mailer.stackfordevs.com
STACKFORDEVS_LLM_API_URL=https://llm.stackfordevs.com
STACKFORDEVS_CMS_API_URL=https://cms.stackfordevs.com
STACKFORDEVS_BILLING_API_URL=https://billing.stackfordevs.com

# Server Configuration
NODE_ENV=production
PORT=3001
